        <input id="ajaxManipulateCss"
               name="ajaxManipulateCss"
               value="{{deviceProfile.get.deviceAttributes.ajaxManipulateCss}}"/>
    </div>
    <div>
        <label for="ajaxSupportEvents">ajaxSupportEvents:</label>
        <input id="ajaxSupportEvents"
               name="ajaxSupportEvents"
               value="{{deviceProfile.get.deviceAttributes.ajaxSupportEvents}}"/>
    </div>
    <div>
        <label for="ajaxSupportEventListener">ajaxSupportEventListener:</label>
        <input id="ajaxSupportEventListener"
               name="ajaxSupportEventListener"
