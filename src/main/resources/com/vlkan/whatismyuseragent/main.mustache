    <div>
        <label for="tablet">tablet:</label>
        <input id="tablet"
               name="tablet"
               value="{{deviceProfile.get.deviceAttributes.tablet}}"/>
    </div>
    <div>
        <label for="desktop">desktop:</label>
        <input id="desktop"
               name="desktop"
               value="{{deviceProfile.get.deviceAttributes.desktop}}"/>
    </div>
    <div>
        <label for="bot">bot:</label>
        <input id="bot"
               name="bot"
               value="{{deviceProfile.get.deviceAttributes.bot}}"/>
    </div>
    <div>
        <label for="ajaxSupportJavaScript">ajaxSupportJavaScript:</label>
        <input id="ajaxSupportJavaScript"
               name="ajaxSupportJavaScript"
               value="{{deviceProfile.get.deviceAttributes.ajaxSupportJavaScript}}"/>
    </div>
    <div>
