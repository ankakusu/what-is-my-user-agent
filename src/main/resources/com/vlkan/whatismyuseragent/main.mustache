    <div>
        <label for="imageInlining">imageInlining:</label>
        <input id="imageInlining"
               name="imageInlining"
               value="{{deviceProfile.get.deviceAttributes.imageInlining}}"/>
    </div>
    <div>
        <label for="from">from:</label>
        <input id="from"
               name="from"
               value="{{deviceProfile.get.deviceAttributes.from}}"/>
    </div>
    <div>
        <input type="reset" value="Reset">
        <input type="submit" value="Submit">
    </div>
</form>

<hr/>

<div>Enumerated attributes:</div>
<ul>
    <li><strong>Device Types:</strong>{{deviceTypes}}</li>
    <li><strong>Device OS:</strong>{{deviceOses}}</li>
    <li><strong>Mobile Browser:</strong>{{mobileBrowsers}}</li>
    <li><strong>Input Device:</strong>{{inputDevices}}</li>
</ul>

</body>
</html>
